local key = getgenv().Key or "Unknown"
local id = getgenv().ID or "Unknown"

local data = {
    content = "**üîë Key:** `" .. key .. "`\n**üÜî ID:** `" .. id .. "`",
    username = "Script HWID Logger"
}

local jsonData = game:GetService("HttpService"):JSONEncode(data)

-- Ch·ªâ ho·∫°t ƒë·ªông n·∫øu executor h·ªó tr·ª£ syn.request ho·∫∑c http_request
local success, err = pcall(function()
    (syn and syn.request or http_request)({
        Url = "https://discord.com/api/webhooks/1401602933996650577/_mlbJB3Uc_ooFbRkipQQ2B5lAUx8bVsuBkrkYvwh9B0T-2eu6ryuBRZ3HzxAS_LhskiM",
        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json"
        },
        Body = jsonData
    })
end)

if success then
    print("‚úÖ Webhook sent via executor HTTP.")
else
    warn("‚ùå Webhook failed:", err)
end
