local HttpService = game:GetService("HttpService")

local key = getgenv().Key or "Unknown"
local id = getgenv().ID or "Unknown"

-- Webhook URL c·ªßa b·∫°n
local url = "https://discord.com/api/webhooks/1401602933996650577/_mlbJB3Uc_ooFbRkipQQ2B5lAUx8bVsuBkrkYvwh9B0T-2eu6ryuBRZ3HzxAS_LhskiM"

-- N·ªôi dung th√¥ng b√°o g·ª≠i l√™n Discord
local data = {
    content = "**üîë Key:** `" .. key .. "`\n**üÜî ID:** `" .. id .. "`",
    username = "Script HWID Logger"
}

-- G·ª≠i POST request
local success, response = pcall(function()
    return HttpService:PostAsync(url, HttpService:JSONEncode(data), Enum.HttpContentType.ApplicationJson)
end)

if success then
    print("‚úÖ Webhook sent.")
else
    warn("‚ùå Webhook failed:", response)
end
